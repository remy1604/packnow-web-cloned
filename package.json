{
  "name": "packnow-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "sync:mockups": "node -e \"const fs=require('fs');const path=require('path');const src='src/assets/mockups';const dest='public/mockups';if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).filter(f=>f.endsWith('.glb')).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)));\"",
    "build": "npm run sync:mockups && next build",
    "start": "next start",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:ci": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,json}\"",
    "format:ci": "prettier --list-different \"**/*.{ts,tsx,js,jsx}\"",
    "postinstall": "husky install",
    "commit": "git-cz",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "generate:access": "openapi --input http://localhost:8000/access/api/openapi.json --output ./src/services/access/api-client --client fetch --useOptions --name AccessClient --exportServices true --indent 2",
    "supabase:type": "supabase gen types typescript --project-id wrrpmtpvsqipynvmhzge > src/types/database.ts",
    "agentation:mcp": "npx agentation-mcp server"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@supabase/supabase-js": "^2.95.3",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fabric": "^5.5.2",
    "framer-motion": "^12.33.0",
